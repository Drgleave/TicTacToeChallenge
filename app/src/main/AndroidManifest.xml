<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.tic_tac_toechallenge">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:fullBackupContent="@xml/backup_descriptor"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TwoPlayerSelectBoard"
            android:screenOrientation="portrait"/>
        <activity android:name=".OnePlayerSelectBoard"
            android:screenOrientation="portrait"/>
        <activity android:name=".Main3SinglePlayerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Main5SinglePlayerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainTwoPlayerBoard3Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Main5x5TwoPlayerActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>